	<div class="text-center">
    <br /> <br />
	<%= image_tag "logo-center.png", size: "200x200", class: "mylogo" %>
	</div><br />
	<h1> Let's Get Fresh </h1><br />
<div class="log-in">
		<div class="form-group inputsmy">
			<%= form_for :session, :url => sessions_path do |f| %>

				<%= f.text_field :email, placeholder: "E-mail", :class => "form-control" %><br />

				<%= f.password_field :password, placeholder: "Password", :class => "form-control" %><br />
			<%= f.submit "Log In", class: 'btn btn-primary' %>
			<% end %>
			</div>
		</div>